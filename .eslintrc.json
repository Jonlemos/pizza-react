{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "babel-eslint",   
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier",
        "prettier/react"   
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier":[ "error", {"endOfLine":"auto"}],
        "react/jsx-filename-extension": [1,{"extensions": [".js", ".jsx"]}]   
    }
}